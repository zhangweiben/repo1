						null, 
						null,
						email_subjectline,
						&apos;TEXT_HTML&apos;,
						email_message
					);
				EXCEPTION
					ELSE
						SET email_exception = CAST(CURRENT_EXCEPTION.message AS LONGVARCHAR);
	 			END;

				BEGIN INDEPENDENT TRANSACTION
