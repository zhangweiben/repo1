	IF (datasourceType = &apos;postgres&apos;) THEN
		SET sqlStatement = 
&apos;CREATE TABLE &quot;{0}&quot;.&quot;&apos;||queueTableDynamicFileQueue||&apos;&quot; (
  &quot;REQUEST_ID&quot;             NUMERIC(19,0),
  &quot;REQUEST_DATE_START&quot;     TIMESTAMP(9),
