PROCEDURE getChildResources(
    IN fullResourcePath 	VARCHAR(4000), 
    IN resourceType 		VARCHAR(255), 
    OUT result CURSOR (
        resourceName VARCHAR(255), 
        resourcePath VARCHAR(4000), 
        resourceType VARCHAR(255), 
        subtype VARCHAR(255), 
        enabled BIT, 
		annotation LONGVARCHAR,
        isNullable VARCHAR(255), 
        columnName VARCHAR(255), 
        columnType VARCHAR(255), 
        nativeBaseType VARCHAR(255), 
        nativeType VARCHAR(255)
        )
)
BEGIN
	open result for 
		select * from /shared/ASAssets/Utilities/repository/getChildResourcesCursor(fullResourcePath, resourceType) 
			where resourceType = &apos;TABLE&apos; OR resourceType = &apos;PROCEDURE&apos;
			and columnName is null;
