						-- Update the trigger
						CALL /shared/ASAssets/Utilities/repository/updateTrigger(
								triggerVect[j].resPath, -- fullResourcePath
								0, 						-- enabled
								&apos;TIMER&apos;, 				-- conditionType
								&apos;PROCEDURE&apos;, 			-- actionType,
								triggerAttrVect,
								NULL, 					-- attributeVect,		
								success,
