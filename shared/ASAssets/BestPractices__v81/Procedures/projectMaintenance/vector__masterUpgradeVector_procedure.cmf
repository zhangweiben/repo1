		,(	7.2, 7.3, &apos;move&apos;, 
			null, null,
			VECTOR[(&apos;$SCRIPTS_PATH_DST/Rebind_Copy_1&apos;,&apos;CONTAINER&apos;)],
			VECTOR[(&apos;$SCRIPTS_PATH_DST/Rebind_Copy_2&apos;,&apos;CONTAINER&apos;)],
