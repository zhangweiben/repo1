PROCEDURE formatLogIdFields(
	-- A series of name/value pair fields that can be used to identify a record in the log
	IN  name1					VARCHAR, 
	IN  value1					VARCHAR(4000),
	IN  name2					VARCHAR, 
	IN  value2					VARCHAR(4000),
	IN  name3					VARCHAR, 
	IN  value3					VARCHAR(4000),
	IN  name4					VARCHAR, 
	IN  value4					VARCHAR(4000),
	IN  name5					VARCHAR, 
	IN  value5					VARCHAR(4000),
	IN  name6					VARCHAR, 
	IN  value6					VARCHAR(4000),
	IN  name7					VARCHAR, 
	IN  value7					VARCHAR(4000),
	IN  name8					VARCHAR, 
	IN  value8					VARCHAR(4000),
	IN  name9					VARCHAR, 
	IN  value9					VARCHAR(4000),
	OUT result					VARCHAR(4000)
)
BEGIN
	SET result = &apos;&apos;;

	IF (value1 IS NOT NULL AND LENGTH(&apos;|&apos;||value1||&apos;|&apos;) &gt; 2) THEN
		set result = result || &apos;{&apos;||TRIM(NVL(name1,&apos;  &apos;))||&apos;=&apos;||TRIM(NVL(value1,&apos;  &apos;))||&apos;} &apos;;
	END IF;
	IF (value2 IS NOT NULL AND LENGTH(&apos;|&apos;||value2||&apos;|&apos;) &gt; 2) THEN
		set result = result || &apos;{&apos;||TRIM(NVL(name2,&apos;  &apos;))||&apos;=&apos;||TRIM(NVL(value2,&apos;  &apos;))||&apos;} &apos;;
	END IF;
	IF (value3 IS NOT NULL AND LENGTH(&apos;|&apos;||value3||&apos;|&apos;) &gt; 2) THEN
		set result = result || &apos;{&apos;||TRIM(NVL(name3,&apos;  &apos;))||&apos;=&apos;||TRIM(NVL(value3,&apos;  &apos;))||&apos;} &apos;;
	END IF;
	IF (value4 IS NOT NULL AND LENGTH(&apos;|&apos;||value4||&apos;|&apos;) &gt; 2) THEN
		set result = result || &apos;{&apos;||TRIM(NVL(name4,&apos;  &apos;))||&apos;=&apos;||TRIM(NVL(value4,&apos;  &apos;))||&apos;} &apos;;
	END IF;
	IF (value5 IS NOT NULL AND LENGTH(&apos;|&apos;||value5||&apos;|&apos;) &gt; 2) THEN
		set result = result || &apos;{&apos;||TRIM(NVL(name5,&apos;  &apos;))||&apos;=&apos;||TRIM(NVL(value5,&apos;  &apos;))||&apos;} &apos;;
	END IF;
	IF (value6 IS NOT NULL AND LENGTH(&apos;|&apos;||value6||&apos;|&apos;) &gt; 2) THEN
		set result = result || &apos;{&apos;||TRIM(NVL(name6,&apos;  &apos;))||&apos;=&apos;||TRIM(NVL(value6,&apos;  &apos;))||&apos;} &apos;;
	END IF;
	IF (value7 IS NOT NULL AND LENGTH(&apos;|&apos;||value7||&apos;|&apos;) &gt; 2) THEN
		set result = result || &apos;{&apos;||TRIM(NVL(name7,&apos;  &apos;))||&apos;=&apos;||TRIM(NVL(value7,&apos;  &apos;))||&apos;} &apos;;
	END IF;
	IF (value8 IS NOT NULL AND LENGTH(&apos;|&apos;||value8||&apos;|&apos;) &gt; 2) THEN
		set result = result || &apos;{&apos;||TRIM(NVL(name8,&apos;  &apos;))||&apos;=&apos;||TRIM(NVL(value8,&apos;  &apos;))||&apos;} &apos;;
	END IF;
	IF (value9 IS NOT NULL AND LENGTH(&apos;|&apos;||value9||&apos;|&apos;) &gt; 2) THEN
		set result = result || &apos;{&apos;||TRIM(NVL(name9,&apos;  &apos;))||&apos;=&apos;||TRIM(NVL(value9,&apos;  &apos;))||&apos;} &apos;;
	END IF;

	IF (result IS NOT NULL AND LENGTH(&apos;|&apos;||result||&apos;|&apos;) &gt; 2) THEN
		set result = &apos;LOG ID::&apos;|| result || &apos;:: &apos;;
	END IF;
END</attribute>
