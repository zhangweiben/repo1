  <attribute name="SQL" type="STRING">SELECT 
	P.rownum, 
	P.sheetRownum, 
	CAST(P.datasource as VARCHAR(255))									datasource,
	CAST(CASE 
		WHEN TRIM(P.sheetName) = &apos;Privileges_shared&apos; 		THEN &apos;1&apos;||P.sheetName
		WHEN TRIM(P.sheetName) = &apos;Privileges_databases&apos; 	THEN &apos;2&apos;||P.sheetName
		WHEN TRIM(P.sheetName) = &apos;Privileges_webservices&apos; 	THEN &apos;3&apos;||P.sheetName
		ELSE TRIM(P.sheetName)
	END as VARCHAR(255))												sheetName, 
	P.Privilege_Type													Privilege_Type,
	CAST(P.Organization as VARCHAR(255))								Organization,  
	CAST(TRIM(P.Project) as VARCHAR(255)) 								Project,
	CAST(TRIM(P.SubProject) as VARCHAR(255))							SubProject,
	CAST(TRIM(CASE
			WHEN INSTR(P.&quot;Resource Path&quot;, &apos;&quot;&apos;) &gt; 0 THEN
				CASE WHEN SUBSTRING(P.&quot;Resource Path&quot;, LENGTH(P.&quot;Resource Path&quot;), 1) = &apos;&quot;&apos; THEN 
					REPLACE(REPLACE(
						REPLACE(REPLACE(SUBSTRING(P.&quot;Resource Path&quot;, 1, LENGTH(P.&quot;Resource Path&quot;) -1), &apos;/&quot;&quot;&apos;, &apos;/&apos;), &apos;&quot;&quot;/&apos;, &apos;/&apos;),
					 &apos;/&quot;&apos;, &apos;/&apos;), &apos;&quot;/&apos;, &apos;/&apos;)
				ELSE 
					REPLACE(REPLACE(
							REPLACE(REPLACE(P.&quot;Resource Path&quot;, &apos;/&quot;&quot;&apos;, &apos;/&apos;), &apos;&quot;&quot;/&apos;, &apos;/&apos;),
					 &apos;/&quot;&apos;, &apos;/&apos;), &apos;&quot;/&apos;, &apos;/&apos;)
				END
			ELSE 
				P.&quot;Resource Path&quot;
		END) AS VARCHAR(4000))											&quot;Resource Path&quot;,
	CAST(TRIM(P.&quot;Resource Type&quot;) AS VARCHAR(255))			 			&quot;Resource Type&quot;,
	CAST(CASE 
		WHEN P.&quot;Recurse Dependencies&quot; IS NULL OR LENGTH(TRIM(P.&quot;Recurse Dependencies&quot;)) = 0 THEN NULL
		WHEN UPPER(TRIM(P.&quot;Recurse Dependencies&quot;)) = &apos;X&apos; THEN &apos;Y&apos;
		WHEN UPPER(TRIM(P.&quot;Recurse Dependencies&quot;)) = &apos;Y&apos; THEN &apos;Y&apos;
		WHEN UPPER(TRIM(P.&quot;Recurse Dependencies&quot;)) = &apos;N&apos; THEN NULL
	    ELSE NULL
	END AS VARCHAR(1))													&quot;Recurse Dependencies&quot;, 
	CAST(CASE 
		WHEN P.&quot;Recurse Dependents&quot; IS NULL OR LENGTH(TRIM(P.&quot;Recurse Dependents&quot;)) = 0 THEN NULL
		WHEN UPPER(TRIM(P.&quot;Recurse Dependents&quot;)) = &apos;X&apos; THEN &apos;Y&apos;
		WHEN UPPER(TRIM(P.&quot;Recurse Dependents&quot;)) = &apos;Y&apos; THEN &apos;Y&apos;
		WHEN UPPER(TRIM(P.&quot;Recurse Dependents&quot;)) = &apos;N&apos; THEN NULL
	    ELSE NULL
	END AS VARCHAR(1))													&quot;Recurse Dependents&quot;, 
	CAST(CASE 
		WHEN P.&quot;Recurse Child&quot; IS NULL OR LENGTH(TRIM(P.&quot;Recurse Child&quot;)) = 0 THEN &apos;N&apos;
	    ELSE UPPER(TRIM(P.&quot;Recurse Child&quot;))
	END AS VARCHAR(3))													&quot;Recurse Child&quot;, 
	CAST(CASE 
		WHEN P.&quot;Revoke All&quot; IS NULL THEN &apos;N&apos;
		WHEN UPPER(TRIM(P.&quot;Revoke All&quot;)) = &apos;X&apos; THEN &apos;Y&apos;
		WHEN UPPER(TRIM(P.&quot;Revoke All&quot;)) = &apos;Y&apos; THEN &apos;Y&apos;
		WHEN UPPER(TRIM(P.&quot;Revoke All&quot;)) = &apos;N&apos; THEN &apos;N&apos;
	    ELSE TRIM(P.&quot;Revoke All&quot;)
	END AS VARCHAR(1))													&quot;Revoke All&quot;,
	CAST(CASE
		WHEN P.Name IS NOT NULL AND LENGTH(TRIM(P.Name)) &gt; 0 AND P.&quot;Domain&quot; IS NOT NULL AND LOWER(P.&quot;Domain&quot;) = &apos;composite&apos; THEN LOWER(P.Name)
		ELSE TRIM(P.Name)
	END AS VARCHAR(255))												Name, 
	CAST(CASE
		WHEN P.&quot;Name Type&quot; IS NULL OR LENGTH(TRIM(P.&quot;Name Type&quot;)) = 0 THEN &apos;GROUP&apos;
		ELSE UPPER(TRIM(P.&quot;Name Type&quot;))
	END AS VARCHAR(255))												&quot;Name Type&quot;,
	CAST(CASE 
		WHEN P.&quot;Domain&quot; IS NULL OR LENGTH(TRIM(P.&quot;Domain&quot;)) = 0 OR LOWER(P.&quot;Domain&quot;) = &apos;composite&apos; THEN &apos;composite&apos;
		ELSE TRIM(P.&quot;Domain&quot;)
	END AS VARCHAR(255))												&quot;Domain&quot;, 
	CAST(UPPER(TRIM(P.&quot;Env Type&quot;)) AS VARCHAR(255))						&quot;Env Type&quot;,
	-- OrderPrecedence: Force privilege rows with ALL NO ACCESS TO OCCUR FIRST since these privileges are used to initialize before setting actual privileges
	CAST(CASE
		WHEN (P.R IS NULL OR LENGTH(TRIM(P.R)) = 0)
		 AND (P.W IS NULL OR LENGTH(TRIM(P.W)) = 0)
		 AND (P.E IS NULL OR LENGTH(TRIM(P.E)) = 0)
		 AND (P.S IS NULL OR LENGTH(TRIM(P.S)) = 0)
		 AND (P.U IS NULL OR LENGTH(TRIM(P.U)) = 0)
		 AND (P.I IS NULL OR LENGTH(TRIM(P.I)) = 0)
		 AND (P.D IS NULL OR LENGTH(TRIM(P.D)) = 0)
		 AND (P.G IS NULL OR LENGTH(TRIM(P.G)) = 0) 
		THEN &apos;A&apos;
		ELSE &apos;Z&apos;
	END AS VARCHAR(1))													OrderPrecedence, 
	CAST(CASE 
		WHEN P.R IS NULL OR LENGTH(TRIM(P.R)) = 0 THEN NULL
		WHEN UPPER(TRIM(P.R)) = &apos;X&apos; THEN &apos;X&apos;
		WHEN UPPER(TRIM(P.R)) = &apos;Y&apos; THEN &apos;X&apos;
		WHEN UPPER(TRIM(P.R)) = &apos;N&apos; THEN NULL
		ELSE NULL
	END AS VARCHAR(1))													R,
	CAST(CASE 
