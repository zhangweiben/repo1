PROCEDURE pPrivileges_All_Excel( 
    OUT result PIPE   /shared/ASAssets/BestPractices_v81/PrivilegeScripts/Helpers/TypeDefinitions.privilegeRowTypeV2
)
BEGIN
	DECLARE moduleName			VARCHAR DEFAULT &apos;pPrivileges_All_Excel&apos;;
	DECLARE debug				CHAR(1) DEFAULT &apos;N&apos;;
	DECLARE rownum				INTEGER DEFAULT 1;
	DECLARE sheetRownum			INTEGER DEFAULT 1;
	DECLARE prevOrganization	VARCHAR DEFAULT &apos;&apos;;
	DECLARE prevSheetName		VARCHAR DEFAULT &apos;&apos;;
	DECLARE rec					/shared/ASAssets/BestPractices_v81/PrivilegeScripts/Helpers/TypeDefinitions.privilegeRowType;

	-- Process the Privileges sheets
	FOR r AS
		SELECT * FROM /shared/ASAssets/BestPractices_v81/PrivilegeScripts/Formatting/Excel/vPrivileges_All_Excel
	DO
		set rec = r;
		-- Set the overall rownum
		set rownum = rownum + 1;
		set rec.rownum = rownum;

		-- set the sheet rownum
		IF (rec.sheetName != prevSheetName) THEN
			set sheetRownum = 1; -- Account for the header row on each sheet
		END IF;
		set sheetRownum = sheetRownum + 1;
		set rec.sheetRownum = sheetRownum;
		-- Save the previous sheet type and sheet name
		set prevOrganization = rec.Organization;
		set prevSheetName = rec.sheetName;

		-- Insert the result into the pipe
		IF (rec.Organization IS NOT NULL AND LENGTH(TRIM(rec.Organization)) &gt; 0 AND
			rec.Project IS NOT NULL AND LENGTH(TRIM(rec.Project)) &gt; 0 AND
			rec.SubProject IS NOT NULL AND LENGTH(TRIM(rec.SubProject)) &gt; 0 AND
			rec.&quot;Resource Path&quot; IS NOT NULL AND LENGTH(TRIM(rec.&quot;Resource Path&quot;)) &gt; 0 AND
			rec.&quot;Env Type&quot; IS NOT NULL AND LENGTH(TRIM(rec.&quot;Env Type&quot;)) &gt; 0 AND
			rec.&quot;Domain&quot; IS NOT NULL AND LENGTH(TRIM(rec.&quot;Domain&quot;)) &gt; 0 AND
			rec.Name IS NOT NULL AND LENGTH(TRIM(rec.Name)) &gt; 0) THEN
				INSERT INTO result VALUES(rec);
		ELSE
			IF (upper(debug) = &apos;Y&apos;) THEN
				call print(moduleName||&apos; : DISCARDING SHEET[&apos;||rec.SheetName||&apos;]  ROW[&apos;||sheetRownum||&apos;]:  &apos;||
					&apos; Organization=[&apos;||TRIM(NVL(rec.Organization,&apos;  &apos;))||&apos;]&apos;||
					&apos; Project=[&apos;||TRIM(NVL(rec.Project,&apos;  &apos;))||&apos;]&apos;||
					&apos; SubProject=[&apos;||TRIM(NVL(rec.SubProject,&apos;  &apos;))||&apos;]&apos;||
					&apos; &quot;Resource Path&quot;=[&apos;||TRIM(NVL(rec.&quot;Resource Path&quot;,&apos;  &apos;))||&apos;]&apos;||
					&apos; &quot;Env Type&quot;=[&apos;||TRIM(NVL(rec.&quot;Env Type&quot;,&apos;  &apos;))||&apos;]&apos;||
					&apos; &quot;Domain&quot;=[&apos;||TRIM(NVL(rec.&quot;Domain&quot;,&apos;  &apos;))||&apos;]&apos;||
					&apos; Name=[&apos;||TRIM(NVL(rec.Name,&apos;  &apos;))||&apos;]&apos;
				);
			END IF;
		END IF;
	END FOR;
END </attribute>
