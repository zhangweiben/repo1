				else
					set message = &apos;No rows exists.&apos;;
				end if;
			end if;
		EXCEPTION
			ELSE
				raise;
		END;

		-- Clean up supporting tables: Delete SUBPROJECT_TYPE and PROJECT_TYPE and ORGANIZATION_TYPE if available and valid to delete.
		if (inSUBPROJECT_NAME is not null) then
