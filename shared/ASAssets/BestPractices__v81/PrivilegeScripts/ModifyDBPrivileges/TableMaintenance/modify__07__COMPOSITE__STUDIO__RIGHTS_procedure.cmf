		-- No matching row was found so insert it.
		IF (NOT duplicate) THEN
			set actionType = &apos;I&apos;;
			IF(performInsertUpdate IS NOT NULL AND UPPER(performInsertUpdate) = &apos;Y&apos;) THEN
				set sqlStatement = 
