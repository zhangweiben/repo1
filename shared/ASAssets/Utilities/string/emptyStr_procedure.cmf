PROCEDURE emptyStr(
	in inputString 		longvarchar,
	out outputString	longvarchar
)
BEGIN
	IF (inputString is null) then
		set outputString = &apos;&apos;;
	else
		set outputString = inputString;
	end if;
END</attribute>
