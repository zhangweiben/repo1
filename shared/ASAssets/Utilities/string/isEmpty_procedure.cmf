PROCEDURE isEmpty(
	IN request LONGVARCHAR,
	OUT response BIT
)
BEGIN
	SET response = 0;
	IF (request IS NULL OR LENGTH(&apos;|&apos;||request||&apos;|&apos;) = 2) THEN
		SET response = 1;
	END IF;
END</attribute>
