PROCEDURE changeResourceOwner(
	IN recurseChildren		BIT,			-- 1=recurse all children,  0=do not recurse.  only set resource path.
	IN resourcePath 		VARCHAR(4096), 	-- The resource path.
	IN resourceType 		VARCHAR(255), 	-- The resource path type: CONTAINER, DATA_SOURCE, TABLE, PROCEDURE, LINK, etc.
	IN newOwner 			VARCHAR(255), 	-- The DV owner name.
	IN newOwnerDomain 		VARCHAR(255) 	-- The DV owner domain.
)
BEGIN
	DECLARE moduleName		VARCHAR DEFAULT &apos;changeResourceOwner&apos;;
	DECLARE debug			CHAR(1) DEFAULT &apos;N&apos;;
	DECLARE recurse			VARCHAR DEfAULT &apos;TRUE&apos;;
	DECLARE requestXML		LONGVARCHAR;
	DECLARE responseXML		XML;
	DECLARE faultXML 		XML;
	DECLARE ex				EXCEPTION;

	IF (recurseChildren = 0) THEN
		SET recurse = &apos;FALSE&apos;;
	END IF;
	SET requestXML = 
		&apos;&lt;ns1:changeResourceOwner xmlns:ns1=&quot;http://www.compositesw.com/services/system/admin/resource&quot;&apos;||CHR(10)||
        &apos;    xmlns:xs=&quot;http://www.w3.org/2001/XMLSchema&quot;&apos;||CHR(10)||
