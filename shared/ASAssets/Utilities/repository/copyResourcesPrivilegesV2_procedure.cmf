	DECLARE pos3, pos4			INTEGER;
	DECLARE pos5, pos6			INTEGER;
	DECLARE len					INTEGER;
	DECLARE resultStr			LONGVARCHAR;
	DECLARE numEntries			INTEGER;
	DECLARE numSrc				INTEGER;
	DECLARE numDst				INTEGER;
	DECLARE ex 					EXCEPTION;
