PROCEDURE updateResourceOwner(
		IN debug			CHAR(1),
        IN resourcePath 	LONGVARCHAR,
        IN resourceType 	VARCHAR,
        IN newOwnerName 	VARCHAR,
        IN newOwnerDomain 	VARCHAR
)
BEGIN
    DECLARE moduleName 				VARCHAR(255) DEFAULT &apos;updateResourceOwner&apos;;-- name of SQL Script
	DECLARE resExists 				BOOLEAN;
	DECLARE requestXML, responseXML, faultXML XML;
	DECLARE exResourceNotFound		EXCEPTION;
	DECLARE exNotNullParameter		EXCEPTION;

	-- Validate parameters
	IF (resourcePath IS NULL) OR (resourceType IS NULL) OR (newOwnerName IS NULL) OR (newOwnerDomain IS NULL) THEN
		RAISE exNotNullParameter VALUE &apos;Required parameter must be provided&apos;;
	END IF;

	-- Validate resource
	CALL /lib/resource/ResourceExists(resourcePath, resourceType, NULL, resExists );
	IF NOT(resExists) THEN
		RAISE exResourceNotFound VALUE &apos;Resource &apos; || resourcePath || &apos; does not exist or the path is invalid.&apos;;
	END IF;

	--Set request sting
	SET requestXML = 
    &apos;&lt;ns1:changeResourceOwner xmlns:ns1=&quot;http://www.compositesw.com/services/system/admin/resource&quot; &apos;||CHR(10)||
    &apos;     xmlns:xs=&quot;http://www.w3.org/2001/XMLSchema&quot; &apos;||CHR(10)||
    &apos;     xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;&gt;&apos;||CHR(10)||
    &apos;     &lt;ns1:path&gt;&apos; || resourcePath || &apos;&lt;/ns1:path&gt;&apos;||CHR(10)||
    &apos;     &lt;ns1:type&gt;&apos; || resourceType || &apos;&lt;/ns1:type&gt;&apos;||CHR(10)||
    &apos;     &lt;ns1:detail&gt;NONE&lt;/ns1:detail&gt;&apos;||CHR(10)||
    &apos;     &lt;ns1:newOwnerName&gt;&apos; || newOwnerName || &apos;&lt;/ns1:newOwnerName&gt;&apos;||CHR(10)||
    &apos;     &lt;ns1:newOwnerDomain&gt;&apos; || newOwnerDomain || &apos;&lt;/ns1:newOwnerDomain&gt;&apos;||CHR(10)||
    &apos;     &lt;ns1:recurse&gt;TRUE&lt;/ns1:recurse&gt;&apos;||CHR(10)||
    &apos;&lt;/ns1:changeResourceOwner&gt;&apos;;

	IF (debug IS NOT NULL AND UPPER(debug) = &apos;Y&apos;) THEN
		CALL   Log(moduleName||&apos;: requestXML=&apos;||NVL(CAST(requestXML AS LONGVARCHAR),&apos;null&apos;));
		CALL Print(moduleName||&apos;: requestXML=&apos;||NVL(CAST(requestXML AS LONGVARCHAR),&apos;null&apos;));
	END IF;

	-- Update the resource ownership
	CALL /services/webservices/system/admin/resource/operations/changeResourceOwner(requestXML, responseXML, faultXML);
	IF faultXML IS NOT NULL THEN
		RAISE exResourceNotFound VALUE CAST(faultXML AS LONGVARCHAR);
	END IF;
END</attribute>
