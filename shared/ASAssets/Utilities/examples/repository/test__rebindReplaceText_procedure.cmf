PROCEDURE test_rebindReplaceText(
	IN testOption		INTEGER,
	IN direction		CHAR(1), -- O=original, C=copy of data source
	OUT success			BIT
)
BEGIN
	DECLARE resourcePath	/shared/ASAssets/Utilities/TypeDefinitions.pathType;
	DECLARE resourceType	VARCHAR;
	DECLARE fromFolder		/shared/ASAssets/Utilities/TypeDefinitions.pathType;
	DECLARE toFolder		/shared/ASAssets/Utilities/TypeDefinitions.pathType;
	DECLARE debug			CHAR(1) DEFAULT &apos;Y&apos;;
	DECLARE ex				EXCEPTION;

	set success = 0;
	if (testOption &lt; 1 or testOption &gt; 6) then
		raise ex value &apos;Invalid test option=&apos;||NVL(CAST(testOption AS VARCHAR),&apos;null&apos;);
	end if;
	if (UPPER(direction) != &apos;C&apos; and UPPER(direction) != &apos;O&apos;) then
		raise ex value &apos;Invalid direction=&apos;||NVL(direction,&apos;null&apos;);
	end if;

	-- VIEW
	if (testOption = 1) then
		SET resourcePath = &apos;/shared/ASAssets/Utilities/examples/repository/source/Rebind/RebindResource/Views/Customers&apos;;
		SET resourceType = &apos;TABLE&apos;;
		if (UPPER(direction) = &apos;C&apos;) then
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_orders&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_ordersCopy&apos;;
		else -- original
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_ordersCopy&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_orders&apos;;
		end if;
	end if;
	-- PROCEDURE
	if (testOption = 2) then
		SET resourcePath = &apos;/shared/ASAssets/Utilities/examples/repository/source/Rebind/RebindResource/Procedures/getCustomerById&apos;;
		SET resourceType = &apos;PROCEDURE&apos;;
		if (UPPER(direction) = &apos;C&apos;) then
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_orders&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_ordersCopy&apos;;
		else -- original
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_ordersCopy&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_orders&apos;;
		end if;
	end if;
	-- PACKAGED QUERY
	if (testOption = 3) then
		SET resourcePath = &apos;/shared/ASAssets/Utilities/examples/repository/source/Rebind/RebindResource/Procedures/packagedCustomerQuery&apos;;
		SET resourceType = &apos;PROCEDURE&apos;;
		if (UPPER(direction) = &apos;C&apos;) then
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_orders&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_ordersCopy&apos;;
		else -- original
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_ordersCopy&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_orders&apos;;
		end if;
	end if;
	-- BASIC XSLT
	if (testOption = 4) then
		SET resourcePath = &apos;/shared/ASAssets/Utilities/examples/repository/source/Rebind/RebindResource/Procedures/basicXSLT&apos;;
		SET resourceType = &apos;PROCEDURE&apos;;
		if (UPPER(direction) = &apos;C&apos;) then
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_XML&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_XMLCopy&apos;;
		else -- original
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_XMLCopy&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_XML&apos;;
		end if;
	end if;
	-- CUSTOM XSLT
	if (testOption = 5) then
		SET resourcePath = &apos;/shared/ASAssets/Utilities/examples/repository/source/Rebind/RebindResource/Procedures/customXSLT&apos;;
		SET resourceType = &apos;PROCEDURE&apos;;
		if (UPPER(direction) = &apos;C&apos;) then
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_XML&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_XMLCopy&apos;;
		else -- original
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_XMLCopy&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_XML&apos;;
		end if;
	end if;
	-- STREAMING XSLT
	if (testOption = 6) then
		SET resourcePath = &apos;/shared/ASAssets/Utilities/examples/repository/source/Rebind/RebindResource/Procedures/streamingXSLT&apos;;
		SET resourceType = &apos;PROCEDURE&apos;;
		if (UPPER(direction) = &apos;C&apos;) then
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_XML&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_XMLCopy&apos;;
		else -- original
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_XMLCopy&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_XML&apos;;
		end if;
	end if;

	call /shared/ASAssets/Utilities/repository/rebind/rebindReplaceText(resourcePath, resourceType, fromFolder, toFolder, debug, success);
END</attribute>
