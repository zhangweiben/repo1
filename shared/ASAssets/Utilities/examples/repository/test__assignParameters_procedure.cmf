			&apos;IS_NULLABLE&apos;,					-- isNullable
			null,							-- attributes
			null							-- annotation
		),
		(	&apos;result&apos;,						-- name
			VECTOR[(						-- sqlType
				&apos;CURSOR (ProductName VARCHAR(50), ProductID INTEGER, ProductDescription VARCHAR(255))&apos;,-- definition
				null,						-- nativeType
