PROCEDURE test_createConsumingViews(
	IN  recurse		BIT,
	IN  overwrite	BIT,
	OUT success		BIT
)
BEGIN
	DECLARE sourcepath 		VARCHAR(2048);
	DECLARE destpath 		VARCHAR(2048);

	set sourcepath = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_inventory/tutorial&apos;;
	set destpath = &apos;/shared/ASAssets/Utilities/examples/repository/target/tutorial&apos;;

	call print (&apos;CREATE CONSUMING VIEWS: Source=&apos;||sourcepath);
	call print (&apos;CREATE CONSUMING VIEWS: Destination=&apos;||destpath);
	call /shared/ASAssets/Utilities/repository/createConsumingViews(sourcepath, destpath, recurse, overwrite, success);
END</attribute>
