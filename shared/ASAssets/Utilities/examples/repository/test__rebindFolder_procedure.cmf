PROCEDURE test_rebindFolder(
	IN testOption		INTEGER,
	IN direction		CHAR(1), -- O=original, C=copy of data source
	OUT success			BIT
)
BEGIN
	DECLARE startingResourceFolder 	/shared/ASAssets/Utilities/TypeDefinitions.pathType;
	DECLARE fromFolder				/shared/ASAssets/Utilities/TypeDefinitions.pathType;
	DECLARE toFolder				/shared/ASAssets/Utilities/TypeDefinitions.pathType;
	DECLARE resourcePath			/shared/ASAssets/Utilities/TypeDefinitions.pathType; 
	DECLARE faultResponse			XML;
	DECLARE ex						EXCEPTION;

	set success = 0;
	if (testOption is null or testOption &lt; 1 or testOption &gt; 3) then
		raise ex value &apos;Invalid test option=&apos;||NVL(CAST(testOption AS VARCHAR),&apos;null&apos;);
	end if;
	if (direction is null or (UPPER(direction) != &apos;C&apos; and UPPER(direction) != &apos;O&apos;)) then
		raise ex value &apos;Invalid direction=&apos;||NVL(direction,&apos;null&apos;);
	end if;
	call print(&apos;-------------------------------&apos;);
	call print(&apos;test option=&apos;||testOption);
	call print(&apos;  direction=&apos;||direction);
	call print(&apos;-------------------------------&apos;);

	-- VIEW
	if (testOption = 1) then
		SET startingResourceFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/Rebind/RebindFolder/Views&apos;;
		if (UPPER(direction) = &apos;C&apos;) then
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_orders&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_ordersCopy&apos;;
		else -- original
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_ordersCopy&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_orders&apos;;
		end if;
	end if;
	-- PROCEDURE
	if (testOption = 2) then
		SET startingResourceFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/Rebind/RebindFolder/Procedures&apos;;
		if (UPPER(direction) = &apos;C&apos;) then
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_orders&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_ordersCopy&apos;;
		else -- original
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_ordersCopy&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_orders&apos;;
		end if;
	end if;
	-- XSLT Procedures
	if (testOption = 3) then
		SET startingResourceFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/Rebind/RebindFolder/XSLTProcedures&apos;;
		if (UPPER(direction) = &apos;C&apos;) then
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_XML&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_XMLCopy&apos;;
		else -- original
			SET fromFolder = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_XMLCopy&apos;;
			SET toFolder =   &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_XML&apos;;
		end if;
	end if;
/*
	IN startingResourceFolder 	/shared/ASAssets/Utilities/TypeDefinitions.pathType,
	IN rebindFromFolder			/shared/ASAssets/Utilities/TypeDefinitions.pathType,
	IN rebindToFolder			/shared/ASAssets/Utilities/TypeDefinitions.pathType,
	IN debug					CHAR(1),
	OUT success					BIT,
	OUT faultResponse			XML
*/
	CALL /shared/ASAssets/Utilities/repository/rebindFolder(startingResourceFolder,fromFolder,toFolder,success,faultResponse);
END</attribute>
