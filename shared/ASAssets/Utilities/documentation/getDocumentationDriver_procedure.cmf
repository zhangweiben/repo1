PROCEDURE getDocumentationDriver(
	IN in_resourcePath				LONGVARCHAR, -- must be a starting path
	IN in_resourceType				VARCHAR,
	IN in_filePath					LONGVARCHAR,
	IN in_docPreambleImpl			LONGVARCHAR,
	IN in_docResourceFormatImpl		LONGVARCHAR,
	IN in_constantPath				LONGVARCHAR,
	IN in_switches					LONGVARCHAR,
	IN in_excludeKeywordsInPathList	LONGVARCHAR,
	IN in_excludePathsList			LONGVARCHAR
)
BEGIN
	DECLARE moduleName					VARCHAR DEFAULT &apos;getDocumentationDriver&apos;;
	-- Location of the constants file
	DECLARE constantPath				VARCHAR(1024) DEFAULT &apos;/shared/ASAssets/Utilities/documentation/constants()&apos;;
	DECLARE debug						CHAR(1);
	DECLARE resourcePath				LONGVARCHAR;
	DECLARE resourceType				VARCHAR;
	DECLARE filePath					LONGVARCHAR;
	DECLARE docPreambleImpl				LONGVARCHAR;
	DECLARE docResourceFormatImpl		LONGVARCHAR;
	DECLARE excludeKeywordsInPathList	LONGVARCHAR;
	DECLARE excludePathsList			LONGVARCHAR;
	DEcLARE switches					LONGVARCHAR;
	DECLARE formattedText				LONGVARCHAR;
	DECLARE ex							EXCEPTION;
	DECLARE message						LONGVARCHAR;
	DECLARE resourceExists				BOOLEAN;

	BEGIN

		-- Validate the constant path is passed in and correct
		if (in_constantPath IS NOT NULL AND LENGTH(&apos;|&apos;||in_constantPath||&apos;|&apos;) &gt; 2) then
			set constantPath = in_constantPath;
		end if;
		if (constantPath IS NULL) then
			raise ex value &apos;The constantPath cannot be null.&apos;;
		else
			call /lib/resource/ResourceExists(constantPath, &apos;PROCEDURE&apos;, null, resourceExists);
			if (NOT resourceExists) then
				raise ex value &apos;The constantPath does not exist=[&apos;||NVL(constantPath,&apos;null&apos;)||&apos;]&apos;;
			end if;
		end if;

		-- Initialize default values
		call /shared/ASAssets/Utilities/string/getConstantV2(constantPath, &apos;debug&apos;, 1, debug);

		-- Determine if the default parameters or incoming parameters should be used.
		if (in_resourcePath IS NOT NULL AND LENGTH(&apos;|&apos;||in_resourcePath||&apos;|&apos;) &gt; 2) then
			set resourcePath = in_resourcePath;
		else
			call /shared/ASAssets/Utilities/string/getConstantV2(constantPath, &apos;resourcePath&apos;, 1, resourcePath);
		end if;

		if (in_resourceType IS NOT NULL AND LENGTH(&apos;|&apos;||in_resourceType||&apos;|&apos;) &gt; 2) then
			set resourceType = in_resourceType;
		else
			call /shared/ASAssets/Utilities/string/getConstantV2(constantPath, &apos;resourceType&apos;, 1, resourceType);
		end if;

		if (in_filePath IS NOT NULL AND LENGTH(&apos;|&apos;||in_filePath||&apos;|&apos;) &gt; 2) then
			set filePath = in_filePath;
		else
			call /shared/ASAssets/Utilities/string/getConstantV2(constantPath, &apos;filePath&apos;, 1, filePath);
		end if;

		if (in_resourcePath IS NOT NULL AND LENGTH(&apos;|&apos;||in_resourcePath||&apos;|&apos;) &gt; 2) then
			set docPreambleImpl = in_docPreambleImpl;
		else
			call /shared/ASAssets/Utilities/string/getConstantV2(constantPath, &apos;docPreambleImpl&apos;, 1, docPreambleImpl);
		end if;

		if (in_docResourceFormatImpl IS NOT NULL AND LENGTH(&apos;|&apos;||in_docResourceFormatImpl||&apos;|&apos;) &gt; 2) then
			set docResourceFormatImpl = in_docResourceFormatImpl;
		else
			call /shared/ASAssets/Utilities/string/getConstantV2(constantPath, &apos;docResourceFormatImpl&apos;, 1, docResourceFormatImpl);
		end if;

		if (in_switches IS NOT NULL AND LENGTH(&apos;|&apos;||in_switches||&apos;|&apos;) &gt; 2) then
			set switches = in_switches;
		else
			call /shared/ASAssets/Utilities/string/getConstantV2(constantPath, &apos;switches&apos;, 1, switches);
		end if;

		if (in_excludeKeywordsInPathList IS NOT NULL AND LENGTH(&apos;|&apos;||in_excludeKeywordsInPathList||&apos;|&apos;) &gt; 2) then
			set excludeKeywordsInPathList = in_excludeKeywordsInPathList;
		else
			call /shared/ASAssets/Utilities/string/getConstantV2(constantPath, &apos;excludeKeywordsInPathList&apos;, 1, excludeKeywordsInPathList);
		end if;

		if (in_excludePathsList IS NOT NULL AND LENGTH(&apos;|&apos;||in_excludePathsList||&apos;|&apos;) &gt; 2) then
			set excludePathsList = in_excludePathsList;
		else
			call /shared/ASAssets/Utilities/string/getConstantV2(constantPath, &apos;excludePathsList&apos;, 1, excludePathsList);
		end if;

		-- Override the switches to always save to a file in intermediate mode as this is the entire point of this driver procedure
		--   However, if the save_file switches are passed in then don&apos;t change them.
		--   Additionally, if the default switches contain the save_file switches then don&apos;t change those either.
		if (POSITION(&apos;save_file=&apos; IN switches) = 0) then
			set switches = switches || &apos; save_file=yes&apos;;
		end if;
		if (POSITION(&apos;save_file_intermediate=&apos; IN switches) = 0) then
			set switches = switches || &apos; save_file_intermediate=yes&apos;;
		end if;

		if (UPPER(debug) = &apos;Y&apos;) then
			call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug,&apos;Starting resourcePath=&apos;||NVL(resourcePath,&apos;null&apos;));
			call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug,&apos;Starting resourceType=&apos;||NVL(resourceType,&apos;null&apos;));
			call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug,&apos;Document constantPath=&apos;||NVL(constantPath,&apos;null&apos;));
			call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug,&apos;Document filePath=&apos;||NVL(filePath,&apos;null&apos;));
			call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug,&apos;Document Preamble Proc=&apos;||NVL(docPreambleImpl,&apos;null&apos;));
			call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug,&apos;Document Format Proc=&apos;||NVL(docResourceFormatImpl,&apos;null&apos;));
			call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug,&apos;Document switches=&apos;||NVL(in_switches,&apos;null&apos;));
			call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug,&apos;Document excludeKeywordsInPathList=&apos;||NVL(excludeKeywordsInPathList,&apos;null&apos;));
			call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,debug,&apos;Document excludePathsList=&apos;||NVL(excludePathsList,&apos;null&apos;));
		end if;

		call /shared/ASAssets/Utilities/documentation/getAllDocumentationAPI(resourcePath, resourceType, docPreambleImpl, docResourceFormatImpl, constantPath, switches, excludeKeywordsInPathList, excludePathsList, filePath, formattedText);
	EXCEPTION
		ELSE
			set message = CAST(CURRENT_EXCEPTION.TRACE AS LONGVARCHAR);
			call /shared/ASAssets/Utilities/log/logDebugMessage(moduleName,&apos;Y&apos;,&apos;Exception occurred: &apos;||NVL(message,&apos;null&apos;));
			raise ex value message;
	END;
END</attribute>
