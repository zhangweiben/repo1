PROCEDURE getFileSeparator(
    OUT result CHAR(1)
)
BEGIN
    SELECT
        &quot;Value&quot;
    INTO
        result
    FROM
        /shared/ASAssets/Utilities/repository/RepoUtils/GetSystemProperties()
    WHERE
        Property = &apos;file.separator&apos;;

END</attribute>
