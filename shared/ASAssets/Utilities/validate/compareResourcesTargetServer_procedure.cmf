			subtype				VARCHAR,
			status				VARCHAR, -- EQUAL, DIFFERENT, TARGET_NOT_EXIST, BOTH_UNREADABLE, SOURCE_UNREADABLE, TARGET_UNREADABLE, UNKNOWN
			sourceNumChars		INTEGER,
			targetNumChars		INTEGER,
