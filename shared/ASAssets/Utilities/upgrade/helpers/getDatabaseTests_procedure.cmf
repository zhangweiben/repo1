	If you would like to obtain assistance with this software, such assistance may be obtained through a separate paid consulting
	agreement with TIBCO.

*/
PROCEDURE getDatabaseTests(
	OUT result			PIPE (
		&quot;Skip Execution&quot;	VARCHAR,
		&quot;Test Name&quot;			VARCHAR(1000),
		&quot;Test Query&quot;		VARCHAR(32768),
		&quot;Test Plan&quot;			VARCHAR(1000),
		&quot;Service Name&quot;		VARCHAR(1000)
	)
)
BEGIN
	DECLARE testQuery		VARCHAR(32768);
	DECLARE columnOrder		VARCHAR(4000);

	FOR r AS 
		SELECT *
		FROM 
		(
		SELECT
			&quot;Skip Execution&quot;,
			&quot;Test Name&quot;,
			&quot;Test Query&quot;,
			&quot;Test Plan&quot;,
			&quot;Service Name&quot;,
			COUNT(COLUMN_ID) column_count
		FROM
			(SELECT 
				&apos;//&apos; &quot;Skip Execution&quot;,
				&apos;T&apos; || T.TABLE_ID &quot;Test Name&quot;,
				&apos;SELECT * FROM &apos; || 
				CASE
					WHEN T.CATALOG_NAME IS NOT NULL THEN
						&apos;&quot;&apos; || T.CATALOG_NAME || &apos;&quot;.&quot;&apos; || T.SCHEMA_NAME || &apos;&quot;.&quot;&apos; || T.TABLE_NAME || &apos;&quot;&apos;
					WHEN T.SCHEMA_NAME IS NOT NULL THEN
						&apos;&quot;&apos; || T.SCHEMA_NAME || &apos;&quot;.&quot;&apos; || T.TABLE_NAME || &apos;&quot;&apos;
					ELSE
						&apos;&quot;&apos; || T.TABLE_NAME || &apos;&quot;&apos;
					END AS &quot;Test Query&quot;,
				&apos;PLAN_&apos; || T.DATASOURCE_NAME &quot;Test Plan&quot;,
				T.DATASOURCE_NAME &quot;Service Name&quot;,
				C.COLUMN_ID
			FROM      
				/services/databases/system/ALL_TABLES T INNER JOIN
				/services/databases/system/ALL_COLUMNS C ON T.TABLE_ID = C.TABLE_ID
			WHERE
				T.DATASOURCE_NAME NOT IN (&apos;system&apos;,&apos;examples&apos;)) d
		GROUP BY
			&quot;Skip Execution&quot;,
			&quot;Test Name&quot;,
			&quot;Test Query&quot;,
			&quot;Test Plan&quot;,
			&quot;Service Name&quot;
		) as DATA
		ORDER BY
			&quot;Test Plan&quot;, &quot;Test Name&quot;
	DO
		CALL /shared/ASAssets/Utilities/upgrade/helpers/returnColumnOrderingString(r.column_count, columnOrder);
		SET testQuery = r.&quot;Test Query&quot; || &apos; &apos; || columnOrder || &apos; FETCH FIRST 10000 ROWS ONLY&apos;;
		INSERT INTO result VALUES (r.&quot;Skip Execution&quot;, r.&quot;Test Name&quot;, testQuery, r.&quot;Test Plan&quot;, r.&quot;Service Name&quot;);
	END FOR;
END</attribute>
