  <attribute name="SQL" type="STRING">SELECT 
    HS2.AMWAY_CNTRY_CD,
    HS2.RGN_DESC,
    HS2.SUB_RGN_DESC,
    HS2.AFF_SHORT_NM,
    HS2.CNTRY_SHORT_NM,
    HS2.BASE_7_ITEM_NO,
    HS2.TO_BASE_7_ITEM_NO,
    HS2.FC_CLASS_CD_STRING,
    HS2.FC_INTRO_CD_STRING,
    HS2.ITEM_DESC,
    HS2.GLBL_BUS_LN_DESC,
    HS2.GLBL_CTGRY_DESC,
	HS2.GLBL_ALT_CTGRY_DESC,
	CASE
		WHEN FC_CLASS_CD_STRING LIKE &apos;%C86%&apos; THEN &apos;HERBALS&apos;
		WHEN GLBL_ALT_CTGRY_DESC = &apos;CORE&apos; AND GLBL_PRDCT_IND_CUT_CD = &apos;G292&apos; THEN &apos;CORE SUPPLEMENT BUNDLES&apos;
		WHEN GLBL_ALT_CTGRY_DESC = &apos;NON-CORE&apos; and GLBL_PRDCT_IND_CUT_CD = &apos;G292&apos; THEN &apos;NON CORE SUPPLEMENT BUNDLES&apos;
		WHEN GLBL_CTGRY_DESC = &apos;KIDS&apos; AND GLBL_PRDCT_IND_CUT_CD = &apos;G292&apos; THEN &apos;KIDS BUNDLES&apos;
		WHEN GLBL_CTGRY_DESC = &apos;BFW&apos; AND GLBL_PRDCT_IND_CUT_CD = &apos;G227&apos; THEN &apos;ALTERNATIVE FORMS&apos;
		WHEN GLBL_CTGRY_DESC = &apos;BFW&apos; AND GLBL_PRDCT_IND_CUT_CD = &apos;G292&apos; THEN &apos;BFW BUNDLES&apos;
		WHEN GLBL_CTGRY_DESC = &apos;ENERGY&apos; AND GLBL_PRDCT_IND_CUT_CD in (&apos;G122&apos;, &apos;G121&apos;) THEN &apos;ENERGY DRINKS&apos;
		WHEN GLBL_CTGRY_DESC = &apos;ENERGY&apos; AND GLBL_PRDCT_IND_CUT_CD IN (&apos;G123&apos;, &apos;G142&apos;, &apos;G144&apos;) THEN &apos;ENERGY SNACKS&apos;
		WHEN GLBL_CTGRY_DESC = &apos;SPORTS NUTRITION&apos; AND GLBL_PRDCT_IND_CUT_CD IN (&apos;G122&apos;, &apos;G227&apos;, &apos;G232&apos;, &apos;G251&apos;) THEN &apos;SPORTS DRINK&apos;
		WHEN GLBL_CTGRY_DESC = &apos;SPORTS NUTRITION&apos; AND GLBL_PRDCT_IND_CUT_CD IN (&apos;G144&apos;, &apos;G145&apos;, &apos;G252&apos;, &apos;G123&apos;, &apos;G142&apos;) THEN &apos;SPORTS SNACK&apos;
		WHEN GLBL_CTGRY_DESC = &apos;SPORTS NUTRITION&apos; AND GLBL_PRDCT_IND_CUT_CD IN (&apos;G236&apos;) THEN &apos;SPORTS PROTEIN&apos;
		WHEN GLBL_CTGRY_DESC = &apos;SPORTS NUTRITION&apos; AND GLBL_PRDCT_IND_CUT_CD IN (&apos;G292&apos;) THEN &apos;SPORTS BUNDLES&apos;
		WHEN GLBL_CTGRY_DESC = &apos;SPORTS NUTRITION&apos; AND GLBL_PRDCT_IND_CUT_CD = &apos;G239&apos; THEN &apos;OTHER VMS&apos;
		WHEN GLBL_CTGRY_DESC = &apos;WEIGHT&apos; AND GLBL_PRDCT_IND_CUT_CD IN (&apos;G200&apos;, &apos;G227&apos;) THEN &apos;MR DRINKS&apos;  
		WHEN GLBL_CTGRY_DESC = &apos;WEIGHT&apos; AND GLBL_PRDCT_IND_CUT_CD IN (&apos;G143&apos;, &apos;G144&apos;, &apos;G145&apos;, &apos;G201&apos;, &apos;G280&apos;) THEN &apos;WEIGHT SNACKS&apos;
		WHEN GLBL_CTGRY_DESC = &apos;WEIGHT&apos; AND GLBL_PRDCT_IND_CUT_CD IN (&apos;G292&apos;) THEN &apos;WEIGHT BUNDLES&apos;
		WHEN GLBL_CTGRY_DESC = &apos;KIDS&apos; THEN &apos;KIDS&apos;
		WHEN GLBL_PRDCT_IND_CUT_CD IN (&apos;G219&apos;, &apos;G220&apos;, &apos;G221&apos;, &apos;G222&apos;, &apos;G255&apos;) THEN &apos;CONVENIENCE PACKS&apos;
		WHEN GLBL_PRDCT_IND_CUT_CD IN (&apos;G139&apos;, &apos;G177&apos;) THEN &apos;FIBER&apos;
		WHEN GLBL_PRDCT_IND_CUT_CD IN (&apos;G76&apos;, &apos;G268&apos;) THEN &apos;EYE HEALTH&apos;
		WHEN GLBL_PRDCT_IND_CUT_CD IN (&apos;G202&apos;, &apos;G152&apos;) THEN &apos;MEMORY&apos;
		WHEN GLBL_PRDCT_IND_CUT_CD IN (&apos;G87&apos;, &apos;G271&apos;, &apos;G78&apos;, &apos;G95&apos;, &apos;G98&apos;, &apos;G106&apos;, &apos;G114&apos;, &apos;G120&apos;, &apos;G151&apos;, &apos;G153&apos;, &apos;G99&apos;, &apos;G206&apos;, &apos;G01&apos;, &apos;G240&apos;, &apos;G248&apos;, &apos;G176&apos;, &apos;G242&apos;, &apos;G231&apos;) THEN &apos;OTHER VMS&apos;
		WHEN GLBL_PRDCT_IND_CUT_CD IN (&apos;G234&apos;, &apos;G235&apos;) THEN &apos;PROTEIN&apos;
		WHEN GLBL_PRDCT_IND_CUT_CD IN (&apos;G178&apos;) THEN &apos;PROBIOTIC&apos;
		WHEN GLBL_PRDCT_IND_CUT_CD IN (&apos;G118&apos;, &apos;G109&apos;, &apos;G101&apos;) THEN &apos;MVM&apos;
		WHEN GLBL_PRDCT_IND_CUT_CD IN (&apos;G297&apos;, &apos;G75&apos;, &apos;G167&apos;) THEN &apos;BEAUTY SUPPLEMENTS&apos;
		WHEN GLBL_PRDCT_IND_CUT_CD IN (&apos;G65&apos;, &apos;G160&apos;, &apos;G138&apos;, &apos;G281&apos;, &apos;G181&apos;, &apos;G97&apos;, &apos;G242&apos;, &apos;G255&apos;) THEN &apos;WEIGHT SUPPLEMENTS&apos;
		WHEN GLBL_PRDCT_IND_CUT_CD IN (&apos;G269&apos;, &apos;G270&apos;, &apos;G272&apos;, &apos;G216&apos;, &apos;G92&apos;, &apos;G179&apos;, &apos;G155&apos;) THEN GLBL_PRDCT_IND_CUT_DESC
		WHEN GLBL_BUS_LN_DESC = &apos;DURABLES&apos; AND GLBL_SUB_CTGRY_DESC IN (&apos;LOCAL PRODUCT&apos;, &apos;PENDING&apos;, &apos;OTHER&apos;) THEN &apos;OTHER DURABLES&apos;
        WHEN GLBL_BUS_LN_DESC = &apos;PERSONAL CARE&apos; AND GLBL_SUB_CTGRY_DESC IN (&apos;LOCAL PRODUCT&apos;, &apos;PENDING&apos;, &apos;OTHER&apos;) THEN &apos;OTHER PERSONAL CARE&apos;
        WHEN GLBL_BUS_LN_DESC = &apos;HOME CARE&apos; AND GLBL_SUB_CTGRY_DESC IN (&apos;LOCAL PRODUCT&apos;, &apos;PENDING&apos;, &apos;OTHER&apos;) THEN &apos;OTHER HOME CARE&apos;
        ELSE GLBL_SUB_CTGRY_DESC
    END AS GLBL_SUB_CTGRY_DESC,
    HS2.GLBL_PRDCT_IND_CUT_DESC,
    HS2.GLBL_PRDCT_FMLY_DESC,
    HS2.GLBL_BRAND_DESC,
    HS2.GLBL_SUB_BRAND_DESC,
    HS2.GLBL_BRAND_FLG,
    HS2.COUNT_SKU,
	HS2.LAST_ORD_DATE
FROM /shared/Common/Business/Logical/Portfolio_Hierarchy/Hierarchy_P1_S2 HS2</attribute>
