  <attribute name="SQL" type="STRING">SELECT 
    HS1.AMWAY_CNTRY_CD, 
    HS1.RGN_DESC, 
    HS1.SUB_RGN_DESC, 
    HS1.AFF_SHORT_NM, 
    HS1.CNTRY_SHORT_NM, 
    HS1.BASE_7_ITEM_NO, 
    HS1.TO_BASE_7_ITEM_NO, 
    HS1.FC_CLASS_CD_STRING, 
    HS1.FC_INTRO_CD_STRING, 
    HS1.ITEM_DESC, 
    HS1.GLBL_BUS_LN_DESC, 
    HS1.GLBL_CTGRY_DESC,
	CASE 
		WHEN GLBL_SUB_CTGRY_DESC IN (&apos;HYDRATING&apos;, &apos;ANTI AGING&apos;, &apos;MASKS&apos;, &apos;BODY&apos;, &apos;SERUMS&apos;, &apos;WHITENING&apos;, &apos;OTHER SKIN NUTRITION&apos;) THEN &apos;SKIN NUTRITION&apos;
		WHEN GLBL_SUB_CTGRY_DESC = &apos;ESSENTIALS&apos; THEN &apos;STUDIO SKIN&apos;
		WHEN GLBL_SUB_CTGRY_DESC = &apos;INTENSIVES&apos; THEN &apos;LABS&apos;
		WHEN GLBL_SUB_CTGRY_DESC = &apos;LUXURY&apos; THEN &apos;LUXURY&apos;
		WHEN GLBL_SUB_CTGRY_DESC = &apos;MENS SKIN CARE&apos; THEN &apos;MENS SKIN CARE&apos;
		WHEN GLBL_SUB_CTGRY_DESC = &apos;SKIN CARE DEVICE&apos; THEN &apos;SKIN CARE DEVICE&apos;
		WHEN GLBL_SUB_CTGRY_DESC = &apos;OTHER SKIN CARE&apos; THEN &apos;OTHER SKIN CARE&apos;
		WHEN GLBL_SUB_CTGRY_DESC = &apos;LOCAL SKIN CARE&apos; THEN &apos;LOCAL SKIN CARE&apos;
		WHEN GLBL_CTGRY_DESC = &apos;SUPPLEMENTS&apos; AND FC_CLASS_CD_STRING LIKE &apos;%C73%&apos; THEN &apos;CORE&apos;
		WHEN GLBL_CTGRY_DESC = &apos;SUPPLEMENTS&apos; AND FC_CLASS_CD_STRING NOT LIKE &apos;%C73%&apos; THEN &apos;NON CORE&apos;
		WHEN GLBL_CTGRY_DESC = &apos;OTHER BEAUTY&apos; THEN &apos;OTHER BEAUTY&apos;
		WHEN GLBL_CTGRY_DESC = &apos;COLOR&apos; AND GLBL_BRAND_DESC NOT IN (&apos;ARTISTRY&apos;) THEN &apos;LOCAL COLOR&apos;
		WHEN GLBL_CTGRY_DESC = &apos;COLOR&apos; AND FC_CLASS_CD_STRING LIKE &apos;%12Z%&apos; THEN &apos;COLOR COLLECTIONS&apos;
		WHEN GLBL_CTGRY_DESC = &apos;COLOR&apos; AND GLBL_BRAND_DESC = &apos;ARTISTRY&apos; AND (FC_CLASS_CD_STRING NOT LIKE &apos;%12Z%&apos; OR FC_CLASS_CD_STRING IS NULL) THEN &apos;CORE COLOR&apos;
		ELSE GLBL_CTGRY_DESC
	END AS GLBL_ALT_CTGRY_DESC,
    HS1.GLBL_SUB_CTGRY_DESC, 
    HS1.GLBL_PRDCT_IND_CUT_CD, 
    HS1.GLBL_PRDCT_IND_CUT_DESC, 
    HS1.GLBL_PRDCT_FMLY_DESC, 
    HS1.GLBL_BRAND_DESC, 
    HS1.GLBL_SUB_BRAND_DESC, 
    CASE 
		WHEN GLBL_BRAND_DESC IN (&apos;ATMOSPHERE&apos;, &apos;ESPRING&apos;, &apos;GLISTER&apos;, &apos;SPREEDENT&apos;, &apos;SATINIQUE&apos;, &apos;G&amp;H&apos;, &apos;BODY SERIES&apos;, &apos;ARTISTRY&apos;, &apos;AMWAY HOME&apos;, &apos;NUTRILITE/NUTRIWAY&apos;, &apos;XS&apos;, &apos;ICOOK&apos;, &apos;AMWAY QUEEN&apos;, &apos;AMWAY AIR&apos;, &apos;AMWAY WATER&apos;, &apos;POSITRIM&apos;) THEN &apos;YES&apos;
        ELSE &apos;NO&apos;
    END AS GLBL_BRAND_FLG, 
    HS1.COUNT_SKU, 
    HS1.LAST_ORD_DATE
FROM      
    /shared/Common/Business/Logical/Portfolio_Hierarchy/Hierarchy_P1_S1 HS1</attribute>
