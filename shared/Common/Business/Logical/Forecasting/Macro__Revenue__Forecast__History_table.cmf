  <attribute name="SQL" type="STRING">/* Script to fetch Macro Revenue Forecast (1 year from current date) and History (1 year before current date) by YearMonth, Country */
SELECT
    Fft001_Macro.calen_yr4 AS fc_calen_yr_key_no
   ,CONCAT(Fft001_Macro.calen_yr4, Fft001_Macro.calen_mo) AS fc_mo_yr_key_no
   ,Fft001_Macro.rev_dist_sales_amt AS revenue_lc
   ,Fft001_Macro.cntry_cd   
   ,Fft101_Corp.corp_cd AS amway_cntry_cd
   ,Fft001_Macro.dcost_amt AS  dc_lc 
   ,Fft001_Macro.dcost_micro_amt  AS dc_plan_lc_fc_price_adj
   
FROM /shared/Common/Physical/Formatting/Forecasting/Fft001_Macro Fft001_Macro
INNER JOIN /shared/Common/Physical/Formatting/Forecasting/Fft101_Corp Fft101_Corp
ON Fft001_Macro.cntry_cd = Fft101_Corp.cntry_cd
AND Fft101_Corp.corp_cd  NOT LIKE &apos;%%1&apos;
WHERE     (Fft001_Macro.calen_yr4 = YEAR(CURRENT_DATE()) - 1 AND Fft001_Macro.calen_mo &gt;= MONTH(CURRENT_DATE())) --Revenue History
	   OR (Fft001_Macro.calen_yr4 = YEAR(CURRENT_DATE()) AND Fft001_Macro.calen_mo &lt;= MONTH(CURRENT_DATE()))   --Revenue History

	   OR (Fft001_Macro.calen_yr4 = YEAR(CURRENT_DATE()) + 1 AND Fft001_Macro.calen_mo &lt;= MONTH(CURRENT_DATE())) --Revenue ForecASt
	   OR (Fft001_Macro.calen_yr4 = YEAR(CURRENT_DATE()) AND Fft001_Macro.calen_mo &gt;= MONTH(CURRENT_DATE()))   --Revenue ForecASt
ORDER BY Fft001_Macro.calen_yr4, Fft001_Macro.calen_mo</attribute>
