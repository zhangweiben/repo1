  <attribute name="SQL" type="STRING">SELECT DISTINCT
    Fft049_Price.corp_cd,
	Fft049_Price.item_no, 
    TRIM(SUBSTRING(Fft049_Price.item_no,1,7)) base_7_item_no, 
    Fft049_Price.calen_yr4, 
    Fft049_Price.calen_mo,
	((Fft049_Price.calen_yr4 * 100) + Fft049_Price.calen_mo) mo_yr_key_no,  
    Fft049_Price.dcost_amt, 
    Fft049_Price.bv_amt, 
    Fft049_Price.nbv_amt, 
    Fft049_Price.pv_amt, 
    Fft049_Price.cmab_amt, 
    Fft049_Price.cmab_pct, 
    Fft049_Price.cmad_amt, 
    Fft049_Price.cmad_pct, 
    Fft049_Price.amab_amt, 
    Fft049_Price.amab_pct, 
    Fft049_Price.amad_amt, 
    Fft049_Price.amad_pct
FROM      
    /shared/Common/Physical/Formatting/Forecasting/Fft049_Price Fft049_Price
WHERE
	((Fft049_Price.calen_yr4 * 100) + Fft049_Price.calen_mo) &lt; CAST(TO_CHAR(CURRENT_DATE, &apos;yyyyMM&apos;) AS INTEGER)
	AND Fft049_Price.dcost_amt &gt; 0
UNION
SELECT DISTINCT
    Appraise.corp_cd,
	Appraise.item_no,
	Appraise.base_7_item_no,
	YEAR(CURRENT_DATE) calen_yr4,
	MONTH(CURRENT_DATE) calen_mo,
	CAST(TO_CHAR(CURRENT_DATE, &apos;yyyyMM&apos;) AS INTEGER) mo_yr_key_no,
    Appraise.dc, 
    Appraise.bv, 
    Appraise.non_bv, 
    Appraise.pv, 
    Appraise.cmab, 
    Appraise.cmab_pct * 100.0, 
    Appraise.cmad, 
    Appraise.cmad_pct * 100.0,
    Appraise.amab, 
    Appraise.amab_pct * 100.0, 
    Appraise.amad, 
    Appraise.amad_pct * 100.0
FROM
	/shared/Common/Business/Business/Pricing/Appraise_All_Prices Appraise
WHERE
	Appraise.dc &gt; 0</attribute>
